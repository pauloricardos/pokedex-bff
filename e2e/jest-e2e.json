{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@domains/(.*)$": "<rootDir>/../src/domains/$1",
    "^@infra/(.*)$": "<rootDir>/../src/infra/$1",
    "^@helpers/(.*)$": "<rootDir>/../src/helpers/$1",
    "^@clients/(.*)$": "<rootDir>/../src/clients/$1"
  }
}
